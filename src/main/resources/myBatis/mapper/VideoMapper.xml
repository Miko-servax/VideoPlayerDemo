<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pky.dao.VideoMapper">

    <select id="getVideo" parameterType="int" resultType="Videos">
        select videoName from videowatcher.videos where videoID=#{videoID}
    </select>

</mapper>